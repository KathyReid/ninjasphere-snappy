{
    "template": "default",
    "policy_vendor": "ubuntu-snappy",
    "policy_version": 1.3,
    "read_path": [
        "@{PROC}/[0-9]*/stat",
        "@{PROC}/",
        "/etc/hosts.allow",
        "/etc/hosts.deny",
	"/etc/passwd",
        "/sys/bus/i2c/devices/0-0050/eeprom"
    ],
    "write_path": [
        "/sys/bus/i2c/devices/i2c-0/new_device"
    ],
    "policy_groups": [
        "networking"
    ]
}
